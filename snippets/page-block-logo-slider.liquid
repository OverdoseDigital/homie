      {% endif %}
      {% if block.settings.image_8 != blank %}
      <div class="logo-img"><img src="{{ block.settings.image_8 | img_url: '1400x' }}" alt="{{ block.settings.image_8.alt | escape }}"></div>
      {% endif %}
      {% if block.settings.image_9 != blank %}
      <div class="logo-img"><img src="{{ block.settings.image_9 | img_url: '1400x' }}" alt="{{ block.settings.image_9.alt | escape }}"></div>
      {% endif %}
      {% if block.settings.image_10 != blank %}
      <div class="logo-img"><img src="{{ block.settings.image_10 | img_url: '1400x' }}" alt="{{ block.settings.image_10.alt | escape }}"></div>
      {% endif %}
    </div>

  </div>
</div>
