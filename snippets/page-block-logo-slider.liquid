{% style %}
  #logo-slider-{{ block.id }} .slider-logo img {
    height: {{ block.settings.img_height }}px;
  }
{% endstyle %}

<div data-subsection data-section-id="{{ block.id }}" data-section-type="video-section" id="logo-slider-{{ block.id }}" class="logo-slider-section">
  <div class="logos-wrap">

    <div class="slider-logo">
      <div class="swiper-container">
        <div class="swiper-wrapper">
          {% if block.settings.image_1 != blank %}
          <div class="swiper-slide"><img src="{{ block.settings.image_1 | img_url: '1400x' }}" alt="{{ block.settings.image_1.alt | escape }}"></div>
          {% endif %}
          {% if block.settings.image_2 != blank %}
          <div class="swiper-slide"><img src="{{ block.settings.image_2 | img_url: '1400x' }}" alt="{{ block.settings.image_2.alt | escape }}"></div>
          {% endif %}
          {% if block.settings.image_3 != blank %}
          <div class="swiper-slide"><img src="{{ block.settings.image_3 | img_url: '1400x' }}" alt="{{ block.settings.image_3.alt | escape }}"></div>
          {% endif %}
          {% if block.settings.image_4 != blank %}
          <div class="swiper-slide"><img src="{{ block.settings.image_4 | img_url: '1400x' }}" alt="{{ block.settings.image_4.alt | escape }}"></div>
          {% endif %}
          {% if block.settings.image_5 != blank %}
          <div class="swiper-slide"><img src="{{ block.settings.image_5 | img_url: '1400x' }}" alt="{{ block.settings.image_5.alt | escape }}"></div>
          {% endif %}
          {% if block.settings.image_6 != blank %}
          <div class="swiper-slide"><img src="{{ block.settings.image_6 | img_url: '1400x' }}" alt="{{ block.settings.image_6.alt | escape }}"></div>
          {% endif %}
          {% if block.settings.image_7 != blank %}
          <div class="swiper-slide"><img src="{{ block.settings.image_7 | img_url: '1400x' }}" alt="{{ block.settings.image_7.alt | escape }}"></div>
          {% endif %}
          {% if block.settings.image_8 != blank %}
          <div class="swiper-slide"><img src="{{ block.settings.image_8 | img_url: '1400x' }}" alt="{{ block.settings.image_8.alt | escape }}"></div>
          {% endif %}
          {% if block.settings.image_9 != blank %}
          <div class="swiper-slide"><img src="{{ block.settings.image_9 | img_url: '1400x' }}" alt="{{ block.settings.image_9.alt | escape }}"></div>
          {% endif %}
          {% if block.settings.image_10 != blank %}
          <div class="swiper-slide"><img src="{{ block.settings.image_10 | img_url: '1400x' }}" alt="{{ block.settings.image_10.alt | escape }}"></div>
          {% endif %}
        </div>
      </div>
      <div class="swiper-button-prev"></div>
      <div class="swiper-button-next"></div>
    </div>
    
    <div class="slider-logo-mobile">
      {% if block.settings.image_1 != blank %}
      <div class="logo-img"><img src="{{ block.settings.image_1 | img_url: '1400x' }}" alt="{{ block.settings.image_1.alt | escape }}"></div>
      {% endif %}
      {% if block.settings.image_2 != blank %}
      <div class="logo-img"><img src="{{ block.settings.image_2 | img_url: '1400x' }}" alt="{{ block.settings.image_2.alt | escape }}"></div>
      {% endif %}
      {% if block.settings.image_3 != blank %}
      <div class="logo-img"><img src="{{ block.settings.image_3 | img_url: '1400x' }}" alt="{{ block.settings.image_3.alt | escape }}"></div>
      {% endif %}
      {% if block.settings.image_4 != blank %}
      <div class="logo-img"><img src="{{ block.settings.image_4 | img_url: '1400x' }}" alt="{{ block.settings.image_4.alt | escape }}"></div>
      {% endif %}
      {% if block.settings.image_5 != blank %}
      <div class="logo-img"><img src="{{ block.settings.image_5 | img_url: '1400x' }}" alt="{{ block.settings.image_5.alt | escape }}"></div>
